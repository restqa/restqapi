{
  "name": "@restqa/restqapi",
  "version": "0.0.13",
  "private": false,
  "description": "REST API Functional Test engine based on Gerkin - RestQa",
  "scripts": {
    "doc": "jsdoc2md  --partial docs/support/scope.hbs --partial docs/support/header.hbs --files src/restqapi/steps/**/index.js > docs/steps-catalog.md",
    "start:example": "export $(cat .env | xargs) && cucumber-js --require ./src/setup-example.js --fail-fast ./sample-project/features-vigie",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@restqa/restqa-plugin-bootstrap": "0.0.4",
    "cucumber": "^6.0.5",
    "dot-object": "^1.6.0",
    "form-data": "^3.0.0",
    "got": "^10.6.0",
    "jsonpath": "^1.0.2",
    "moment": "^2.18.1",
    "uuid": "^3.4.0"
  },
  "main": "src/restqapi/index.js",
  "devDependencies": {
    "eslint": "^7.16.0",
    "jest": "^25.2.7",
    "jest-runner-eslint": "^0.10.0",
    "jsdoc-to-markdown": "^5.0.3",
    "nodemon": "^2.0.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "jest": {
    "projects": [
      {
        "displayName": "test"
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "lint",
        "testMatch": [
          "<rootDir>/src/**/*.js"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restqa/restqapi.git"
  },
  "keywords": [
    "api",
    "rest",
    "restapi",
    "test",
    "cucumber",
    "restqa"
  ],
  "author": "Olivier Rodomond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/restqa/restqapi/issues"
  },
  "homepage": "https://github.com/restqa/restqapi#readme"
}
